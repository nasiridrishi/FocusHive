{
  "request": {
    "method": "POST",
    "url": "/send",
    "headers": {
      "Content-Type": {
        "equalTo": "application/json"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "messageId": "mock_email_message_id",
      "status": "sent",
      "timestamp": "{{now format='yyyy-MM-dd HH:mm:ss'}}",
      "deliveryTime": "{{now format='yyyy-MM-dd HH:mm:ss'}}"
    }
  }
}